generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model project {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  project_type  String
  project_name  String
  live_link     String
  github_link   String
  thumbnail_url String
  description   String
  images        String[]
  techs         String[]
  visited       Int
  position      Int
}
